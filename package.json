{
  "name": "rust-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.9.0"
  },
  "scripts": {
    "web:dev": "yarn workspace @rust-server-query/web dev",
